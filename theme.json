{
  "document": {
    "block_prefix": "\n",
    "block_suffix": "\n",
    "color": "252",
    "margin": 4
  },
  "block_quote": {
    "indent": 1,
    "indent_token": "│ ",
    "color": "244"
  },
  "paragraph": {},
  "list": {
    "level_indent": 2
  },
  "heading": {
    "block_suffix": "\n",
    "bold": true
  },
  "h1": {
    "prefix": "# ",
    "color": "#d19a66",
    "bold": true
  },
  "h2": {
    "prefix": "## ",
    "color": "#61afef"
  },
  "h3": {
    "prefix": "### ",
    "color": "#98c379"
  },
  "h4": {
    "prefix": "#### ",
    "color": "#c678dd"
  },
  "h5": {},
  "h6": {},
  "text": {},
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "italic": true,
    "color": "#e5c07b"
  },
  "strong": {
    "bold": true,
    "color": "#e06c75"
  },
  "hr": {
    "color": "240",
    "format": "\n────────────────────────────────────────────────────────\n"
  },
  "item": {
    "block_prefix": "• "
  },
  "enumeration": {
    "block_prefix": ". "
  },
  "task": {
    "ticked": "[✓] ",
    "unticked": "[ ] "
  },
  "link": {
    "color": "#61afef",
    "underline": true
  },
  "link_text": {
    "color": "#56b6c2",
    "bold": true
  },
  "image": {
    "color": "212",
    "underline": true
  },
  "image_text": {
    "color": "243",
    "format": "Image: {{.text}} →"
  },
  "code": {
    "prefix": " ",
    "suffix": " ",
    "color": "#98c379",
    "background_color": "236"
  },
  "code_block": {
    "theme": "onedark",
    "margin": 2
  },
  "table": {
    "center_separator": "┼",
    "column_separator": "│",
    "row_separator": "─"
  },
  "definition_list": {},
  "definition_term": {},
  "definition_description": {
    "block_prefix": "\n→ "
  },
  "html_block": {},
  "html_span": {}
}
